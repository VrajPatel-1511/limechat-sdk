{
  "name": "flits/limechat-sdk",
  "description": "An Limechat-sdk for php project",
  "license": "MIT",
  "authors": [
    {
      "name": "Milind",
      "email": "milind@getflits.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Flits\\Limechat\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "type": "composer",
            "url": "https://packages.composer.getflits.com"
        }
    ],
    "require-dev": {
      "composer/composer": "^1.9",
      "fzaninotto/faker": "dev-master",
      "phpunit/phpunit": "^9.0",
      "facade/ignition": "^2.3.6",
      "nunomaduro/collision": "^5.0",
      "mockery/mockery": "2.0.x-dev"
  }
}
